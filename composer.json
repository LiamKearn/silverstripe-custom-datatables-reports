{
    "name": "liamkearn/silverstripe-custom-datatables-reports",
    "description": "Custom reporting for SilverStripe using JQuery",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "datatables",
        "JQuery",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "LiamKearn\\CustomReport\\": "src/",
            "LiamKearn\\CustomReport\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
